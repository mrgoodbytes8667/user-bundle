{
    "name": "mrgoodbytes8667/user-bundle",
    "description": "User management commands inspired by FOSUserBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/annotations": "^1.0 | ^2.0",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/orm": "^2.8",
        "mrgoodbytes8667/command-bundle": "^1.2.2 | ^2.0",
        "symfony/console": "^6.2",
        "symfony/framework-bundle": "^6.2",
        "symfony/property-access": "^6.2",
        "symfony/property-info": "^6.2",
        "symfony/security-core": "^6.2",
        "symfony/validator": "^6.2"
    },
    "require-dev": {
        "mrgoodbytes8667/test-common": ">=0.0.23",
        "mrgoodbytes8667/test-common-faker": "^0.3.4",
        "rector/rector": "^0.16",
        "phpunit/phpunit": "^9.6",
        "symfony/test-pack": "^1.0.8"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@mrgoodbytes.dev"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "phpunit/phpunit": "<9.5 >=10",
        "symfony/serializer": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\UserBundle\\": "src/",
            "Bytes\\UserBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
